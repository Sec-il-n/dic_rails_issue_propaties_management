<h1>物件一覧</h1>
<table>
  <thead>
    <tr>
      <th>物件名</th>
      <th>賃料</th>
      <th>住所</th>
      <th>築年数</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @properties.each do |property| %>
    <tr>
        <td><%= property.name %></td>
        <td><%= property.price %>円</td>
        <td><%= property.address %></td>
        <td><%= property.age %>年</td>
        <td><%= property.note %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to '物件を登録する', new_property_path %>
